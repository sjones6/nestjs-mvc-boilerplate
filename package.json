    "test": "mocha ./src/**/*.spec.ts",
    "test:watch": "mocha ./src/**/*.spec.ts --watch",
    "test:debug": "node --inspect-brk ./node_modules/mocha/bin/_mocha ./src/**/*.spec.ts --watch",
    "test:e2e": "mocha ./src/**/*.e2e.ts --file=./test/setup.e2e.ts --exit"
